{
    "name": "easycorp/easyadmin-bundle",
    "type": "symfony-bundle",
    "description": "Admin generator for Symfony applications",
    "keywords": ["backend", "admin", "generator"],
    "homepage": "https://github.com/EasyCorp/EasyAdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Project Contributors",
            "homepage": "https://github.com/EasyCorp/EasyAdminBundle/blob/master/CONTRIBUTORS.md"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "doctrine/common": "^3.4",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/orm": "^3.2",
        "doctrine/persistence": "^3.3",
        "pagerfanta/core": "^4.6",
        "pagerfanta/doctrine-orm-adapter": "^4.6",
        "symfony/asset": "^6.4|^7.0",
        "symfony/cache": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/doctrine-bridge": "^6.4|^7.0",
        "symfony/error-handler": "^6.4|^7.0",
        "symfony/event-dispatcher": "^6.4|^7.0",
        "symfony/expression-language": "^6.4|^7.0",
        "symfony/finder": "^6.4|^7.0",
        "symfony/form": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-foundation": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/polyfill-mbstring": "^1.7",
        "symfony/property-access": "^6.4|^7.0",
        "symfony/security-bundle": "^6.4|^7.0",
        "symfony/translation": "^6.4|^7.0",
        "symfony/twig-bridge": "^6.4|^7.0",
        "symfony/twig-bundle": "^6.4|^7.0",
        "symfony/validator": "^6.4|^7.0",
        "twig/twig": "^3.10"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.7",
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "psr/log": "^1|^2|^3",
        "symfony/browser-kit": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "symfony/css-selector": "^6.4|^7.0",
        "symfony/dom-crawler": "^6.4|^7.0",
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "symfony/var-dumper": "^6.4|^7.0",
        "symfony/yaml": "^6.4|^7.0"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "symfony/doctrine-bridge": "source",
            "*": "dist"
        }
    },
    "autoload": {
        "psr-4": {
            "EasyCorp\\Bundle\\EasyAdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTestBundle\\": "tests/Fixtures/AppTestBundle/",
            "EasyCorp\\Bundle\\EasyAdminBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
